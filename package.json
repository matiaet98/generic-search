{
  "name": "generic-search",
  "version": "1.0.0",
  "description": "Este proyecto apunta a crear una pantalla generica de consulta, donde el usuario pueda customizar los campos a mostrar, los filtros, operaciones sobre el universo resultante (aggregations como sumar, contar, promedio, etc).",
  "main": "index.js",
  "directories": {
    "test": "test",
    "bin": "dist",
    "doc": "docs"
  },
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register",
    "start": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build": "./node_modules/webpack/bin/webpack.js -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matiaet98/generic-search.git"
  },
  "keywords": [
    "generic",
    "search"
  ],
  "author": "matiaet98",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/matiaet98/generic-search/issues"
  },
  "homepage": "https://github.com/matiaet98/generic-search#readme",
  "devDependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.6",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-datepicker": "^0.34.0",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "semantic-ui-react": "^0.60.4"
  }
}
